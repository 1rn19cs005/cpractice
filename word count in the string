#include<stdio.h>
#include<string.h>
#include<stdio.h>

int main(){
    char ch[100];
    printf("enter the string:\n");
    gets(ch);
    int count=0;
    for(int i=0;i<strlen(ch);i++){
        int ascii;
        ascii=(int)ch[i];
        if(ascii==32)
            count++;
    }
    count++;
    printf("toatl no. of words:%d", count);
    return 0;
}
